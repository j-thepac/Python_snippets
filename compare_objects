"""
To compare 2 objects using magic methods
"""

class Person():
    def __init__(self,height):
        self.height=height
    def __eq__(self,obj):
        if (self.height==obj.height):
            print("equal")
        else:print("no")
        

         
p1=Person(10)
p2=Person(20)
p1==p2
#no

